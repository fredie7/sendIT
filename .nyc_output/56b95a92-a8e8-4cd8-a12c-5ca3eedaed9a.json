M,CAACG,GAAP,CAAW,UAACC,GAAD;AAAA,eAASA,GAAG,CAACC,GAAb;AAAA,OAAX,EAA6B,CAA7B,CAAnB;AACA,aAAOd,GAAG,CAACe,MAAJ,CAAW,GAAX,EAAgBC,IAAhB,CAAqB;AAAEC,QAAAA,KAAK,EAAEN;AAAT,OAArB,CAAP;AACD;;AACDV,IAAAA,IAAI;AACL;AA1Be,CAAlB;eA4BeJ,S","sourceRoot":"C:\\Users\\matthew\\Documents\\goal\\sendit_proj\\sendit_backend\\validator","sourcesContent":["const validator = {\n  signupValidator: (req, res, next) => {\n    req.check('email', 'fill in email field').notEmpty();\n    req.check('email').isLength({ min: 4, max: 150 }).withMessage('email must contain between 4 - 150 characters');\n    req.check('email').matches(/.+@.+/).withMessage('email must contain an @ symbol');\n    req.check('password').isLength({ min: 6 }).withMessage('password must contain at least 6 characters');\n    req.check('password').matches(/\\d/).withMessage('password must contain digit character');\n    const errors = req.validationErrors();\n    if (errors) {\n      const firstError = errors.map((err) => err.msg)[0];\n      return res.status(422).json({ error: firstError });\n    }\n    next();\n  },\n  signinValidator: (req, res, next) => {\n    req.check('email', 'fill in email field').notEmpty();\n    req.check('email').isLength({min: 4, max: 150 }).withMessage('email must contain between 4 - 150 characters');\n    req.check('email').matches(/.+@.+/).withMessage('email must contain an @ symbol');\n    req.check('password').isLength({ min: 6 }).withMessage('password must contain at least 6 characters');\n    req.check('password').matches(/\\d/).withMessage('password must contain digit character');\n    const errors = req.validationErrors();\n    if (errors) {\n      const firstError = errors.map((err) => err.msg)[0];\n      return res.status(422).json({ error: firstError });\n    }\n    next();\n  },\n};\nexport default validator;\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"8894c87bc4ce525a44ddcbaac67a46fa2a5a6030","contentHash":"546acebb14580c1f71e23bd7da0cfc2a676c0490a0f921