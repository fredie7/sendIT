dy exists' });\n    }\n    const newUser = {\n      name: req.body.name,\n      email: req.body.email,\n      password: req.body.password,\n      id: newID(),\n    };\n    data.push(newUser);\n    return res.status(201).json(newUser);\n  },\n\n  signin: (req, res) => {\n    const existingUser = data.find((user) => user.email === req.body.email && user.password === req.body.password);\n    if (!existingUser) {\n      return res.status(401).json({ error: 'user does not exist' });\n    }\n    const { email } = req.body.email;\n    const token = jwt.sign({ email }, jwtSecretKey, {\n      expiresIn: jwtExpiryTime,\n    });\n    return res.status(200).json({ token });\n  },\n};\nexport default authController;\n"]},"_coverageSchema":"1a1c01bbd47fc00a2c39e90264f33305004495a9","hash":"c9943921e2c720917458f953746567b84efc53c5","contentHash":"d618b4e14f33fe6394af7a2c7d633a9f3ccf073010139065ad240932af9b6501"},"C:\\Users\\matthew\\Documents\\goal\\sendit_proj\\sendit_backend\\data\\users.js":{"path":"C:\\Users\\matthew\\Documents\\goal\\sendit_proj\\sendit_backend\\data\\users.js","statementMap":{"0":{"start":{"line":3,"column":0},"end":{"line":5,"column":3}},"1":{"start":{"line":6,"column":0},"end":{"line":6,"column":28}},"2":{"start":{"line":7,"column":11},"end":{"line":22,"column":2}},"3":{"start":{"line":23,"column":15},"end":{"line":23,"column":19}},"4":{"start":{"line":24,"column":0},"end":{"line":24,"column":30}}},"fnMap":{},"branchMap":{},"s":{"0":1,"1":1,"2":1,"3":1,"4":1},"f":{},"b":{},"inputSourceMap":{"version":3,"sources":["users.js"],"names":["data","id","name","email","password"],"mappings":";;;;;;AAAA,IAAMA,IAAI,GAAG,CACX;AACEC,EAAAA,EAAE,EAAE,CADN;AAEEC,EAAAA,IAAI,EAAE,OAFR;AAGEC,EAAAA,KAAK,EAAE,iBAHT;AAIEC,EAAAA,QAAQ,EAAE;AAJZ,CADW,EAOX;AACEH,EAAAA,EAAE,EAAE,CADN;AAEEC,EAAAA,IAAI,EAAE,OAFR;AAGEC,EAAAA,KAAK,EAAE,iBAHT;AAIEC,EAAAA,QAAQ,EAAE;AAJZ,CAPW,EAaX;AACEH,EAAAA,EAAE,EAAE,CADN;AAEEC,EAAAA,IAAI,EAAE,QAFR;AAGEC,EAAAA,KAAK,EAAE,kBAHT;AAIEC,EAAAA,QAAQ,EAAE;AAJZ,