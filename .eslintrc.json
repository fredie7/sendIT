{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console" : "off",
        // "import/newline-after-import" : "off",
        // "no-unused-vars" : 0,
        // "max-len" : 0
        "consistent-return": 0
    }
}